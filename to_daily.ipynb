    "import xarray as xr\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "for y in np.arange(2021, 2022):\n",
    "    data = xr.open_dataset(f'./z500/data_z500_{y}.nc')\n",
    "    data_daily = data.resample(time='1D').mean()\n",
    "    data_daily.to_netcdf(f'./z500/data_z500_{y}_daily.nc')"
   ]
  },
  {
   "cell_type": "code",
